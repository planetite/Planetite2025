generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  password String
}

model PatchNote {
  id    Int     @id @default(autoincrement())
  version String
  releaseDate DateTime @default(now())
  text  String
}